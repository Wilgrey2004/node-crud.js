<main>
  <div class="container">
    <h1 class="title">Productos</h1>
  </div>
  <section class="productos">
    <% json.forEach(productos => {%>
    <article>
      <h2><%= productos.title %></h2>
      <p><%= productos.description %></p>
    </article>
    <%}) %>
  </section>
</main>

